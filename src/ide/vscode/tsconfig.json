{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2017",
    "outDir": "out",
    "lib": [
      "ES2017"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}
