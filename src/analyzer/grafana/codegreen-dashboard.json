{
  "dashboard": {
    "id": null,
    "title": "CodeGreen Energy Monitoring Dashboard",
    "tags": ["codegreen", "energy", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Energy Consumption by Session",
        "type": "stat",
        "targets": [
          {
            "expr": "codegreen_session_total_energy_joules",
            "legendFormat": "{{session_id}} ({{language}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            },
            "unit": "joule"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Session Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "codegreen_session_duration_seconds",
            "legendFormat": "{{session_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 60
                },
                {
                  "color": "red",
                  "value": 300
                }
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Energy Consumption Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "codegreen_checkpoint_energy_joules",
            "legendFormat": "{{function_name}} ({{file_path}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "joule"
          }
        },
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Energy by Function",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (function_name) (codegreen_checkpoint_energy_joules)",
            "legendFormat": "{{function_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "unit": "joule"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 5,
        "title": "Energy by Programming Language",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum by (language) (codegreen_session_total_energy_joules)",
            "legendFormat": "{{language}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "continuous-GrYlRd"
            },
            "custom": {
              "displayMode": "gradient",
              "orientation": "horizontal"
            },
            "mappings": [],
            "max": 100,
            "min": 0,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "unit": "joule"
          }
        },
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 6,
        "title": "Top Energy Consuming Files",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (file_path) (codegreen_checkpoint_energy_joules))",
            "legendFormat": "{{file_path}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "joule"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 30
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
    "links": []
  }
}